package objectSize;

public interface IsaacSize {
	int COLUMNGAP = 3;	// 열간격
	int ROWGAP = 6;	// 행간격
	int HEADWIDTH = 28;	// 머리 가로 길이
	int HEADHEIGHT = 25;	// 머리 세로 길이
	int BODYWIDTH = 20;	// 몸 가로 길이
	int BODYHEIGHT = 15;	// 몸 세로 길이
	int TOTALWIDTH = 20;	// 머리, 몸 가로 길이
	int TOTALHEIGHT = 15;	// 머리, 몸 세로 길이
}
